          <argument>
            <kind>1</kind>
            <string>///DISEGNA SCHERMATA
//palette
col_pal_two[0] = make_colour_rgb(97,124,115);   //sailor's blue
col_pal_two[1] = make_colour_rgb(236,197,102);  //butter
col_pal_two[2] = make_colour_rgb(254,142,44);   //orange
col_pal_two[3] = make_colour_rgb(254,142,44);   //earth
col_pal_two[4] = make_colour_rgb(37,12,7);      //qblack

//schermate di errore
var xcenter = display_get_gui_width()/2;
var ycenter = display_get_gui_height()/2;
var half_wlen_string = 0;
var half_hlen_string = ds_list_size(global.mex_error)/2;
var max_len = 0;
var me = "";
for(var i=0; i&lt;ds_list_size(global.mex_error); i++)
{
    me += global.mex_error[| i] + "#";
    if(string_length(global.mex_error[| i]) &gt; max_len)
    {
        max_len = string_length(global.mex_error[| i]);
        half_wlen_string = max_len/2;
    }
}

draw_set_halign(fa_center);
draw_set_valign(fa_middle);
draw_set_colour(col_pal_two[1]); //4
draw_rectangle(xcenter -16*half_wlen_string, ycenter -32*half_hlen_string, xcenter +16*half_wlen_string, ycenter +32*half_hlen_string, false);
draw_set_colour(col_pal_two[4]); //3
draw_text(xcenter, ycenter, me);

draw_set_halign(fa_left);
draw_set_valign(fa_top);

</string>
          </argument>
        </arguments>
