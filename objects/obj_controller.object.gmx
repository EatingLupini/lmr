d3d_draw_wall(room_width +dist, room_height +dist, +dist, -dist, room_height +dist, -dist, background_get_texture(tex_skybox_back), 1, 1);

//mountain
d3d_draw_wall(room_width +dist -50, -dist, 1024, room_width +dist -50, room_height +dist, 0, background_get_texture(tex_mount), 10, 1); //right
d3d_draw_wall(room_width +dist, room_height +dist -50, 1024, -dist, room_height +dist -50, 0, background_get_texture(tex_mount), 10, 1); //back
